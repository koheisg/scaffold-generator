{
  "name": "s-generator",
  "version": "0.0.1",
  "description": "In browser, Scaffld Generater generate scaffold command of ruby on rails.",
  "main": "app.js",
  "dependencies": {
    "bootstrap-sass": "^3.3.6",
    "eslint": "^4.19.1",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.0",
    "stringify": "^5.1.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^8.0.0",
    "browser-sync": "^2.12.8",
    "browserify": "^16.2.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "watch": "^1.0.2",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "node-sass --include-path ./node_modules/bootstrap-sass/assets/stylesheets/ sass/application.scss > dist/build.css",
    "js": "watchify -t babelify ./src/app.js -t stringify ./templates/*.html -d -o ./dist/build.js -v",
    "lint": "eslint ./src/",
    "watch": "watch 'npm run lint' ./src/",
    "serve": "browser-sync start --server ./ --files ./dist/**/* --server dist",
    "start": "npm run watch & npm run js & npm run css & npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koheisg/scaffold-generator.git"
  },
  "author": "koheisg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koheisg/scaffold-generator/issues"
  },
  "homepage": "https://github.com/koheisg/scaffold-generator"
}
