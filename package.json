{
  "name": "s-generator",
  "version": "0.0.1",
  "description": "In browser, Scaffld Generater generate scaffold command of ruby on rails.",
  "main": "app.js",
  "dependencies": {
    "babel-preset-react": "^6.24.1",
    "eslint": "^2.10.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browser-sync": "^2.24.3",
    "browserify": "^13.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "watch": "^0.18.0",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "js": "watchify -t babelify ./src/app.js -d -o ./dist/build.js -v",
    "lint": "eslint ./src/",
    "watch": "watch 'npm run lint' ./src/",
    "serve": "browser-sync start --server ./ --files ./dist/**/* --server dist",
    "start": "npm run watch & npm run js & npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/koheisg/scaffold-generator.git"
  },
  "author": "koheisg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/koheisg/scaffold-generator/issues"
  },
  "homepage": "https://github.com/koheisg/scaffold-generator"
}
